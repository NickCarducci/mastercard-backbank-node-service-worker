{
  "name": "mastercard-api-core",
  "version": "1.4.19",
  "description": "Core functionality for MasterCard API",
  "main": "index.js",
  "scripts": {
    "jenkins-setup": "npm run setup-keystore && npm run setup-tests && npm run setup-testResults",
    "setup-keystore": "rm -rf ./keystore && cp -r ./sdk-core-nodejs-private/keystore ./keystore",
    "setup-testResults": "rm -rf ./testResults && mkdir testResults",
    "setup-tests": "rm -rf ./test && cp -r ./sdk-core-nodejs-private/test ./test",
    "test": "mocha --reporter spec",
    "testJenkins": "mocha > testResults/test-report.xml",
    "testCoverage": "istanbul cover _mocha"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Mastercard/sdk-core-nodejs.git"
  },
  "keywords": [
    "MasterCard",
    "API"
  ],
  "author": "MasterCard Worldwide <APISupport@mastercard.com> (https://developer.mastercard.com)",
  "license": "BSD-2-Clause",
  "devDependencies": {
    "istanbul": "0.4.5",
    "mocha": "2.5.3",
    "mock-require": "1.3.0",
    "mockery": "1.7.0",
    "rewire": "2.5.2",
    "should": "8.4.0",
    "sinon": "1.17.6",
    "sleep": "3.0.1",
    "chai": "3.5.0",
    "chai-string": "1.4.0"
  },
  "dependencies": {
    "node-forge": "^0.6.39",
    "request": "^2.81.0"
  }
}
